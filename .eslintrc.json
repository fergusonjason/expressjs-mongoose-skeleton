{
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "max-len": [1, 120, 2, {"ignoreComments": true}],
        "quotes":["backtick","doublequote"]
    }
}